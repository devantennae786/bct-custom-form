{
  "name": "react-formal",
  "version": "2.0.0-1",
  "description": "Classy HTML form management for React",
  "main": "lib/index.cjs.js",
  "modules": "es/index.js",
  "author": {
    "name": "Jason Quense",
    "email": "monastic.panic@gmail.com"
  },
  "homepage": "http://jquense.github.io/react-formal/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/react-formal"
  },
  "license": "MIT",
  "files": [
    "lib",
    "es"
  ],
  "keywords": [
    "react-formal",
    "react",
    "form",
    "forms",
    "inputs",
    "validator",
    "schema",
    "validation",
    "react-component",
    "yup"
  ],
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "setupFilesAfterEnv": [
      "./test/index.js"
    ]
  },
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint src",
    "docs": "yarn --cwd www start",
    "docs:deploy": "yarn --cwd www deploy",
    "build:esm": " babel src --out-dir es --delete-dir-on-start --env-name esm",
    "build": "babel src --out-dir lib --delete-dir-on-start --copy-files && npm run build:esm",
    "prepublishOnly": "npm run build",
    "release": "rollout"
  },
  "peerDependencies": {
    "react": ">=16.4.0"
  },
  "dependencies": {
    "@restart/hooks": "^0.3.14",
    "chain-function": "^1.0.0",
    "classnames": "^2.2.5",
    "invariant": "^2.2.4",
    "lodash": "^4.17.15",
    "memoize-one": "^5.1.1",
    "prop-types": "^15.7.2",
    "prop-types-extra": "^1.1.0",
    "property-expr": "^1.4.0",
    "react-context-toolbox": "^2.0.2",
    "shallowequal": "^1.1.0",
    "topeka": "^4.2.0",
    "uncontrollable": "^7.0.1",
    "warning": "^4.0.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@4c/rollout": "^1.4.0",
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.8.0",
    "babel-preset-jason": "^6.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-jason": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.15.1",
    "jest": "^24.9.0",
    "react": "^16.10.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.10.1",
    "react-formal": "^0.28.2",
    "react-tackle-box": "^2.1.0",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0"
  }
}
