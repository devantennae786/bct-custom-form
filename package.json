{
  "name": "react-formal",
  "version": "0.29.0-13",
  "description": "Classy HTML form management for React",
  "main": "lib/index.cjs.js",
  "modules": "es/index.js",
  "author": {
    "name": "Jason Quense",
    "email": "monastic.panic@gmail.com"
  },
  "homepage": "http://jquense.github.io/react-formal/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/react-formal"
  },
  "license": "MIT",
  "files": [
    "lib",
    "es"
  ],
  "keywords": [
    "react-formal",
    "react",
    "form",
    "forms",
    "inputs",
    "validator",
    "schema",
    "validation",
    "react-component",
    "yup"
  ],
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "setupTestFrameworkScriptFile": "./test/index.js"
  },
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint src",
    "docs": "webpack --config docs/webpack.config.js --progress",
    "docs:watch": "webpack-dev-server --config docs/webpack.config.js",
    "build:esm": " babel src --out-dir es --delete-dir-on-start --env-name esm",
    "build": "babel src --out-dir lib --delete-dir-on-start --copy-files && npm run build:esm",
    "prepublishOnly": "npm run build",
    "release": "rollout"
  },
  "peerDependencies": {
    "react": ">=16.4.0"
  },
  "dependencies": {
    "chain-function": "^1.0.0",
    "classnames": "^2.2.5",
    "invariant": "^2.2.4",
    "lodash": "^4.17.10",
    "memoize-one": "^4.0.2",
    "prop-types": "^15.6.1",
    "prop-types-extra": "^1.1.0",
    "property-expr": "^1.4.0",
    "react-context-toolbox": "^1.2.2",
    "shallowequal": "^1.1.0",
    "topeka": "^3.1.0",
    "uncontrollable": "^6.0.0",
    "warning": "^4.0.1",
    "yup": "^0.26.3"
  },
  "devDependencies": {
    "@4c/rollout": "^1.1.0",
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-preset-jason": "^6.0.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-config-jason": "^4.0.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^23.6.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-formal": "^0.28.2",
    "react-tackle-box": "^2.1.0",
    "rimraf": "^2.5.2",
    "sinon": "^6.2.0",
    "sinon-chai": "^3.1.0"
  }
