{
  "name": "react-formal",
  "version": "2.2.3",
  "description": "Classy HTML form management for React",
  "main": "lib/cjs/index.cjs.js",
  "types": "lib/cjs/index.d.ts",
  "module": "lib/esm/index.js",
  "author": {
    "name": "Jason Quense",
    "email": "monastic.panic@gmail.com"
  },
  "homepage": "http://jquense.github.io/react-formal/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jquense/react-formal.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "lib",
    "es"
  ],
  "keywords": [
    "react-formal",
    "react",
    "form",
    "forms",
    "inputs",
    "validator",
    "schema",
    "validation",
    "react-component",
    "yup"
  ],
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "setupFilesAfterEnv": [
      "./test/index.js"
    ]
  },
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "lint": "eslint src",
    "docs": "yarn --cwd www start",
    "docs:deploy": "yarn --cwd www deploy",
    "build:pick": "cherry-pick --cwd=lib --input-dir=../src --cjs-dir=cjs --esm-dir=esm",
    "build": "rimraf lib && 4c build src && yarn build:pick",
    "prepublishOnly": "npm run build",
    "deploy-docs": "yarn --cwd www build --prefix-paths && gh-pages -d www/public",
    "release": "rollout"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "quoteProps": "consistent"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "release": {
    "conventionalCommits": true
  },
  "peerDependencies": {
    "react": ">=16.4.0"
  },
  "dependencies": {
    "@restart/hooks": "^0.3.25",
    "@types/lodash": "^4.14.165",
    "@types/react": ">=16.14.0",
    "@typescript-eslint/parser": "^4.9.0",
    "classnames": "^2.2.5",
    "invariant": "^2.2.4",
    "lodash": "^4.17.20",
    "memoize-one": "^5.1.1",
    "prop-types": "^15.7.2",
    "property-expr": "^2.0.4",
    "shallowequal": "^1.1.0",
    "topeka": "^4.4.1",
    "uncontrollable": "^7.1.1",
    "warning": "^4.0.3",
    "yup": ">=0.32.3"
  },
  "devDependencies": {
    "@4c/cli": "^2.1.12",
    "@4c/rollout": "^2.1.11",
    "@4c/tsconfig": "^0.3.1",
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.16",
    "@types/sinon": "^9.0.9",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-preset-jason": "^6.3.0",
    "cherry-pick": "^0.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-4catalyzer-typescript": "^3.0.1",
    "eslint-config-jason": "^7.0.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-tackle-box": "^2.1.0",
    "react-widgets": "^5.0.0-beta.15",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  },
  "bugs": {
    "url": "https://github.com/jquense/react-formal/issues"
  }
}
